<h1> Index Page Test </h1>
<%= link_to 'New User', new_user_path, class: 'btn btn-default' %>
<table>
  <tr>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %> </td>
        <td><%= user.fname %> </td>
        <td><%= user.lname %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
