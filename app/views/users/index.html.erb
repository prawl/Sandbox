<%= link_to 'Back', root_path, class: 'btn btn-primary' %>

<h1>Users Page </h1>
<%= link_to 'New User', new_user_path, class: 'btn btn-primary' %>
<table>
  <tr>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.email, user_path(user) %>
        <td><%= user.fname %> </td>
        <td><%= user.lname %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
